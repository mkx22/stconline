  <process id="Entrust" name="entrust" isExecutable="true">
    <startEvent id="start">
      <extensionElements>
        <activiti:formProperty id="EntrustID" name="the ID of the trust" type="String" required="true"/>
      </extensionElements>
    </startEvent>
    <userTask id="submitentrust" name="TobeSubmit" activiti:assignee="${ClientID}" >
      <extensionElements>
        <activiti:formProperty id="user" name="client" type="string" value="client"/>
        <modeler:allow-send-email><![CDATA[true]]></modeler:allow-send-email>
        <modeler:initiator-can-complete><![CDATA[false]]></modeler:initiator-can-complete>
        <modeler:editor-resource-id><![CDATA[sid-AB045AD0-DCD5-45B2-ACD1-948752FB8A4E]]></modeler:editor-resource-id>
      </extensionElements>
    </userTask>
    <userTask id="reviewentrust" name="TobeReview" activiti:candidateGroups="${WorkerIDs}">
      <extensionElements>
       <!-- <activiti:taskListener event="create"
                               class="com.njustctiviti.TaskListenerlmpl"></activiti:taskListener>-->
        <activiti:formProperty id="reviewentrustapproval" name="Do you approve this request?" type="enum" required="true"/>
        <activiti:formProperty id="user" name="worker" type="string" value="worker"/>
        <activiti:formProperty id="reviewentrustcomments" name="reason about pass or reject the entrust" type="string" required="true"/>
        <modeler:allow-send-email><![CDATA[true]]></modeler:allow-send-email>
        <modeler:initiator-can-complete><![CDATA[false]]></modeler:initiator-can-complete>
        <modeler:editor-resource-id><![CDATA[sid-BF1DA17F-62CD-42D0-8FB2-7C1AF61A3050]]></modeler:editor-resource-id>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="gate1">
