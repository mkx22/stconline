    </exclusiveGateway>
    <endEvent id="end">
      <extensionElements>
        <modeler:editor-resource-id><![CDATA[sid-54A1159E-A89D-48E2-9387-F2D7C89BCD40]]></modeler:editor-resource-id>
      </extensionElements>
    </endEvent>
    <sequenceFlow id="f1" sourceRef="start" targetRef="submitentrust">
      <extensionElements>
        <modeler:editor-resource-id><![CDATA[sid-DF3EC220-C74A-4280-91B6-DDB4BF1B0CED]]></modeler:editor-resource-id>
      </extensionElements>
    </sequenceFlow>
    <sequenceFlow id="f2" sourceRef="submitentrust" targetRef="reviewentrust">
      <extensionElements>
        <modeler:editor-resource-id><![CDATA[sid-747CB676-4CC8-413C-B4A7-25FCE3B87866]]></modeler:editor-resource-id>
      </extensionElements>
    </sequenceFlow>
    <sequenceFlow id="f4" name="通过" sourceRef="gate1" targetRef="end">
      <extensionElements>
        <modeler:editor-resource-id><![CDATA[sid-D43AE9A4-86AA-4C13-B9CF-8FAF5660FEC5]]></modeler:editor-resource-id>
      </extensionElements>
     <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewentrustapproval == 'ReviewPass'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="f5" name="不通过" sourceRef="gate1" targetRef="submitentrust">
      <extensionElements>
        <modeler:editor-resource-id><![CDATA[sid-4791BF60-AD89-40AA-820E-EECED7315C5F]]></modeler:editor-resource-id>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewentrustapproval == 'ReviewReject'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="f3" sourceRef="reviewentrust" targetRef="gate1">
